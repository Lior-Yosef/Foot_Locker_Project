@using Foot_Locker_Project.Models;
<link href="~/Content/Tablecss.css" rel="stylesheet" type="text/css" />


<table>

    <tr>
        <th>Id </th>
        <th>ShoesType</th>
        <th>CompanyName </th>
        <th>Type</th>
        <th>Price </th>
        <th>Quantity </th>
        <th>OnSale </th>
        <th>LinkIMG</th>
    </tr>
    @foreach (Shoe item in Model)
    {
        <tr>
            <td>@item.Id </td>
            <td>@item.ShoesType </td>
            <td>@item.CompanyName </td>
            <td>@item.Type </td>
            <td>@item.Price </td>
            <td>@item.Quantity </td>
            <td>@item.OnSale </td>
            <td><img src="@item.LinkIMG" </td>

        </tr>
    }




</table>